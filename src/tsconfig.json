{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "importHelpers": true,
    "declaration": true,
    "sourceMap": false,
    "target": "es6",
    "lib": [
      "es6",
      "dom"
    ],
    "outDir": "../lib",
    "types": [
      "assert",
      "faker",
      "mocha",
      "node",
      "pluralize",
      "bunyan",
      "chalk",
      "fs-extra",
      "mysql2"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ]
  }
}